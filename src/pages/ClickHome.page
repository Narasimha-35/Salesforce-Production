<script>
 $(document).ready(function ()
    {
        $('.clsMemberLogin').click(function () 
        {
          $('#MemberLoginPopUp').removeClass('displaynone');          
          $('.messageCell').css('display','none');
          $('[id$=member_login_strmsg]').remove();
          $('[id$=current_lender_strmsg]').remove();
