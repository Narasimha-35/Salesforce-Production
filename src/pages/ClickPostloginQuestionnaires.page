  $(function() { 
   $('.popupDatepicker').datepick({
     dateFormat: 'dd/mm/yyyy',
    changeMonth: true,
    yearRange : '1930:2025',
    changeYear: true});
   $('#inlineDatepicker').datepick({onSelect: showDate});
  });  
 
   function showDate(date1) {
     $('[id$=schdateId]').val(date1);
   }
 
  function slide()
  {
    //alert($('[id$=ipthdn]').val());
    if($('[id$=ipthdn]').val() === 'true')
    {
       $('.slider-arrow--right').trigger('click');
    }
    if($('[id$=ipthdndependent]').val()>0)
    {
        for(var i=0;i<($('[id$=ipthdndependent]').val()-1);i++)
        {   
            $('.slider-arrow--right').trigger('click');
        }   
    }   
    if($('[id$=ipthdnISDependentSkip]').val() === 'true')
    {
       $('.slider-arrow--right').trigger('click');
    }
    if($('[id$=idhdnMasterQues]').val() == 0){
       //  $('#goBackLinkId').addClass('displaynone');  
        $('[id$=goBackLinkId]').css('display','none');
    }else{
            $('[id$=goBackLinkId]').css('display','inline-block');
         }
    dependentQuesdatepick();
  }
  function Backslide()
  {
    //alert('TEST'+$('[id$=ipthdndependent]').val());
    if($('[id$=ipthdn]').val() === 'true')
    {
        //alert('single');
       $('.slider-arrow--left').trigger('click');
    }
    if($('[id$=ipthdndependent]').val()>0)
    {
        //alert('multiple');
        for(var i=0;i<($('[id$=ipthdndependent]').val()-1);i++)
        {   
            $('.slider-arrow--left').trigger('click');
        }   
    }
    if($('[id$=ipthdnISDependentSkip]').val() === 'true')
    {
       $('.slider-arrow--left').trigger('click');
    }
    if($('[id$=idhdnMasterQues]').val() == 0){
       //  $('#goBackLinkId').addClass('displaynone');  
        $('[id$=goBackLinkId]').css('display','none');
    }
    dependentQuesdatepick();
  }
  
  function dependentQuesdatepick()
  {
      $('.popupDepeDatepicker').datepick({
     dateFormat: 'dd/mm/yyyy',
    changeMonth: true,
    yearRange : '1930:2025',
    changeYear: true});
   $('.popupDepeDatepicker').click(function ()
    {
      var txtoffset = $('.dateInlineClass').offset(); 
        //alert('top'+txtoffset.top);
       // alert('left'+ $('.datepick').offset());
        $('.datepick').offset({ top: txtoffset.top+538});
        $('.datepick').offset({ left: txtoffset.left+110});
    }); 
  }
  
  function showReqPopup(strParentType,strChildType)
  {
      // alert('hai'+strChildType);
       //var strparent = strParentType;
       $('[id$=hdnparentTypeId]').val(strParentType);
       $('[id$=hdnChildTypeId]').val(strChildType);
       if(strParentType === 'Assets')
       {
         $('[id$=assetHeadingId]').css('display','block');
         $('[id$=liabilityHeadingId]').css('display','none');
         $('[id$=assetActionBtnId]').css('display','block');
         $('[id$=liabActionBtnId]').css('display','none');
       } else{
                $('[id$=assetHeadingId]').css('display','none');
                $('[id$=liabilityHeadingId]').css('display','block');
                $('[id$=assetActionBtnId]').css('display','none');
                $('[id$=liabActionBtnId]').css('display','block');
            }   
       //resetPopup(); 
       $('[id$=cmdResetPnl]').trigger('click');
       $('#reqcallPopId').css('display','block');
       
  }
  
  $(document).ready(function ()
  {
    //alert('hello'+($('[id$=iptotalQuesSlide]').val()));
    for(var i=0;i<($('[id$=iptotalQuesSlide]').val());i++)
    {
       $('.slider-arrow--right').trigger('click');
    } 
    for(var i=0;i<($('[id$=ipthdnIndex]').val());i++)
    {
       $('.slider-arrow--right').trigger('click');
    }   
    if('{!IsSlideToNextQues}' === 'true')
    {
        $('.slider-arrow--right').trigger('click');
    }
    if($('[id$=idhdnMasterQues]').val() == 0){
       //  $('#goBackLinkId').addClass('displaynone');  
        $('[id$=goBackLinkId]').css('display','none');
    }
   /* $('[id$=btnAsset]').click(function(e)
    {
       setTimeout(AssetsAndLiablities, 3000);                
       setTimeout(test, 5000);
    });*/
    
    
    $('.clsOnlynumeric').keypress(function(e) 
    {
        var keyCode;
        if (window.event) // IE
        {
            keyCode = e.keyCode;
        }
        else if (e.which) // Netscape/Firefox/Opera
        {
            keyCode = e.which;
        }
        
        if (keyCode == 46 && $(this).val().indexOf(".") < 1) {
            return true;
        }
        
        return ((!((keyCode < 48) || (keyCode > 57))) || (keyCode == 8));
    }); 
    
    $('.popupDatepicker').click(function ()
    {
        var txtoffset = $('.dateInlineClass').offset(); 
        //$('.datepick').offset({ top: txtoffset.top+50});
        //$('.datepick').offset({ left: txtoffset.left+5});
       $('.datepick').offset({ top: txtoffset.top+168});
        $('.datepick').offset({ left: txtoffset.left+110});
    });
    $('#reqCallFadedId').click(function () 
    {
        $('#reqcallPopId').css('display','none');
    });
    $('#reqPopCloseId').click(function () 
     { 
