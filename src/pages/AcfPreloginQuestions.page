                                          $('input[id$=textId]').attr('autocomplete','off');
                                          
                                          $('[id$=textId]').keyup(function(){
                                                  //alert($(this).val()); 
                                                  queryData($(this).val());
                                          });
                                          $('.clsOnlynumeric').keypress(function(e) 
                                            {
                                                var keyCode;
                                                if (window.event) // IE
                                                {
                                                    keyCode = e.keyCode;
                                                }
                                                else if (e.which) // Netscape/Firefox/Opera
                                                {
                                                    keyCode = e.which;
                                                }
                                        
                                                return ((!((keyCode < 48) || (keyCode > 57))) || (keyCode == 8));
                                            });
