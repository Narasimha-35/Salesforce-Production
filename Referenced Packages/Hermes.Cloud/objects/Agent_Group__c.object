        <fullName>Apply_Appli_to_agents</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Apply Programs</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/apex.js&quot;)}
var res = sforce.apex.execute(&quot;vhc/Ctrl07Group_ApplySettings&quot;,&quot;ApplyAppliOnAgents&quot;, {customerOid:&quot;{!Customer__c.Oid__c}&quot;, agentGroupID:&quot;{!Agent_Group__c.Group_Id__c}&quot;, agentGroupSfID:&quot;{!Agent_Group__c.Id}&quot;}); 

if(res == &apos;true&apos;)
       window.alert(&quot;{!$Label.Apply_Settings_Success}&quot; );
else
       window.alert(&quot;{!$Label.Apply_Settings_Failed}&quot; );</url>
    </webLinks>
