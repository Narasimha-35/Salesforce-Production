    <description>Members with the most posts and comments who are now inactive. These members haven&apos;t logged in to the community in the last 7 days, but have logged in once during the last 30 days. This report includes only members with more than 5 posts or 5 comments.</description>
    <filter>
        <booleanFilter>(1 AND 2 AND (3 OR 4))</booleanFilter>
        <criteriaItems>
            <column>ChatterActivity$Parent.LastLoginDate</column>
            <operator>lessThan</operator>
            <value>LAST_N_DAYS:7</value>
        </criteriaItems>
        <criteriaItems>
            <column>ChatterActivity$Parent.IsActive</column>
            <operator>equals</operator>
            <value>1</value>
        </criteriaItems>
        <criteriaItems>
            <column>ChatterActivity$CommentCount</column>
            <operator>greaterThan</operator>
            <value>5</value>
        </criteriaItems>
        <criteriaItems>
            <column>ChatterActivity$PostCount</column>
            <operator>greaterThan</operator>
            <value>5</value>
        </criteriaItems>
    </filter>
    <format>Tabular</format>
    <name>Contributors Inactive For 7+ Days</name>
    <params>
        <name>co</name>
        <value>1</value>
    </params>
    <reportType>Network_Member_Activity_1_0__c</reportType>
    <scope>organization</scope>
    <showDetails>true</showDetails>
    <sortColumn>ChatterActivity$Parent.LastLoginDate</sortColumn>
    <sortOrder>Asc</sortOrder>
    <timeFrameFilter>
        <dateColumn>ChatterActivity$Parent.LastLoginDate</dateColumn>
        <interval>INTERVAL_LAST30</interval>
    </timeFrameFilter>
</Report>
