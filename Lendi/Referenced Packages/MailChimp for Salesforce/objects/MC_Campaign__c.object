{!REQUIRESCRIPT(&quot;/soap/ajax/26.0/apex.js&quot;)}

try {
	var response = sforce.apex.execute(&quot;MC4SF.Util&quot;, &quot;loadCampaigns&quot;, {id: &apos;{!MC_Campaign__c.MailChimp_ID__c}&apos;});

	if (response != &apos;Success&apos;) {
